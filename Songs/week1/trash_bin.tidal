d1 $ qtrigger $ n " 0 1 3 [4*2]" # sound "808"# room 2 # sz 0.7


d2 $ qtrigger $ n (scale "minor" "0 ~ ~ [0 -1]") #sound "kick" # room 1 # sz 0.5 

d3 $ n (scale "minor" "0*2 [-5 -3] [~ ~] [-7*2]") #sound "supersaw" #octave 2 # room 1 # sz 0.7 # gain 1

do
    let mylist = [0,-5,-3,-7,0,-8,-2,-7,3,-5]
    let l = pure (length mylist)
    let pat = segment 8 $ (mylist !!) <$> (mod (sig floor) (l))
    d4 $ fast 4 $ s "supersaw" # scale "minor" pat|+"a" #octave 2 # room 1 # sz 0.7

hush 
